; not C1 : not (a <= 0 or b <= 0 or c <= 0)
; not C3 : not (a == b and b == c)
; not C2 : a + b > c and a + c > b and b + c > a
; C4 : (a == b or b == c or a == c)
(declare-const a Int)
(declare-const b Int)
(declare-const c Int)
(assert (> a 0))
(assert (> b 0))
(assert (> c 0))
(assert (not (and (= a b) (= b c))))
(assert (> (+ a b) c))
(assert (> (+ a c) b))
(assert (> (+ b c) a))
(assert (or (or (= a b) (= b c)) (= a c)))
(check-sat)
(get-model)